lookup kernHorizontalKerninginLatinloo {
  lookupflag 0;
    pos \L \quoteright -266;
    pos \L \quotedblright -350;
    pos \c \k -38;
    pos \c \h -43;
    pos \e \x -31;
    pos \f \quoteright 42;
    pos \f \quotedblright 56;
} kernHorizontalKerninginLatinloo;

feature kern {

 script latn;
     language dflt ;
      lookup kernHorizontalKerninginLatinloo;
} kern;
